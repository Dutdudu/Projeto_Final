<?xml version="1.0" ?>
<sdf version="1.10">
    <world name="projeto_final_world">
        <physics name="1ms" type="ignored">
            <max_step_size>0.001</max_step_size>
            <real_time_factor>1.0</real_time_factor>
        </physics>
        <plugin
            filename="gz-sim-physics-system"
            name="gz::sim::systems::Physics">
        </plugin>
        <plugin
            filename="gz-sim-user-commands-system"
            name="gz::sim::systems::UserCommands">
        </plugin>
        <plugin
            filename="gz-sim-scene-broadcaster-system"
            name="gz::sim::systems::SceneBroadcaster">
        </plugin>

        <light type="directional" name="sun">
            <cast_shadows>true</cast_shadows>
            <pose>0 0 10 0 0 0</pose>
            <diffuse>0.8 0.8 0.8 1</diffuse>
            <specular>0.2 0.2 0.2 1</specular>
            <attenuation>
                <range>1000</range>
                <constant>0.9</constant>
                <linear>0.01</linear>
                <quadratic>0.001</quadratic>
            </attenuation>
            <direction>-0.5 0.1 -0.9</direction>
        </light>

        <model name="ground_plane">
            <static>true</static>
            <link name="link">
                <collision name="collision">
                <geometry>
                    <plane>
                    <normal>0 0 1</normal>
                    </plane>
                </geometry>
                </collision>
                <visual name="visual">
                <geometry>
                    <plane>
                    <normal>0 0 1</normal>
                    <size>100 100</size>
                    </plane>
                </geometry>
                <material>
                    <ambient>0.8 0.8 0.8 1</ambient>
                    <diffuse>0.8 0.8 0.8 1</diffuse>
                    <specular>0.8 0.8 0.8 1</specular>
                </material>
                </visual>
            </link>
        </model>
        <model name="alvo1">
            <self_collide>true</self_collide>
            <pose>5 7 0.5 0 0 0</pose>
            <include>
            <uri>model://../models/alvo.sdf</uri>
            </include>
        </model>
        <model name="alvo2">
            <self_collide>true</self_collide>
            <pose>6 -6 0.5 0 0 0</pose>
            <include>
            <uri>model://../models/alvo.sdf</uri>
            </include>
        </model>
        <model name="alvo3">
            <self_collide>true</self_collide>
            <pose>9 0 0.5 0 0 0</pose>
            <include>
            <uri>model://../models/alvo.sdf</uri>
            </include>
        </model>
        <model name="alvo4">
            <self_collide>true</self_collide>
            <pose>-6 3 0.5 0 0 0</pose>
            <include>
            <uri>model://../models/alvo.sdf</uri>
            </include>
        </model>
        <model name="alvo5">
            <self_collide>true</self_collide>
            <pose>-2 -6 0.5 0 0 0</pose>
            <include>
            <uri>model://../models/alvo.sdf</uri>
            </include>
        </model>
        <model name="carrinho">
            <self_collide>true</self_collide>
            <pose>0 0 0 0 0 0</pose>
            <link name='chassis'>
                <pose>0 0 0.2 0 0 0</pose>
                <inertial> <!--inertial properties of the link mass, inertia matrix-->
                    <mass>1.14395</mass>
                    <inertia>
                        <ixx>0.103909</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.145854</iyy>
                        <iyz>0</iyz>
                        <izz>0.232603</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <box>
                            <size>1.2 1.0 0.3</size>
                        </box>
                    </geometry>
                    <!--let's add color to our link-->
                    <material>
                        <ambient>0.7 0.7 0.9 1</ambient>
                        <diffuse>0.7 0.7 0.9 1</diffuse>
                        <specular>0.7 0.7 0.9 1</specular>
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <box>
                            <size>1.2 1.0 0.3</size>
                        </box>
                    </geometry>
                </collision>
            </link>
            <link name='left_wheel'>
                <pose relative_to="chassis">-0.4 0.6 0 -1.5707 0 0</pose>
                <inertial>
                    <mass>1</mass>
                    <inertia>
                        <ixx>0.013333</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.013333</iyy>
                        <iyz>0</iyz>
                        <izz>0.02</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <cylinder>
                            <radius>0.2</radius>
                            <length>0.2</length>
                        </cylinder>
                    </geometry>
                    <material>
                        <ambient>0.5 0.5 0.7 1</ambient>
                        <diffuse>0.5 0.5 0.7 1</diffuse>
                        <specular>0.5 0.5 0.7 1</specular>
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <cylinder>
                            <radius>0.2</radius>
                            <length>0.2</length>
                        </cylinder>
                    </geometry>
                </collision>
            </link>
            <link name='right_wheel'>
                <pose relative_to="chassis">-0.4 -0.6 0 -1.5707 0 0</pose> <!--angles are in radian-->
                <inertial>
                    <mass>1</mass>
                    <inertia>
                        <ixx>0.013333</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.013333</iyy>
                        <iyz>0</iyz>
                        <izz>0.02</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <cylinder>
                            <radius>0.2</radius>
                            <length>0.2</length>
                        </cylinder>
                    </geometry>
                    <material>
                        <ambient>0.5 0.5 0.7 1</ambient>
                        <diffuse>0.5 0.5 0.7 1</diffuse>
                        <specular>0.5 0.5 0.7 1</specular>
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <cylinder>
                            <radius>0.2</radius>
                            <length>0.2</length>
                        </cylinder>
                    </geometry>
                </collision>
            </link>
            <link name='back_left_wheel'>
                <pose relative_to="chassis">0.4 0.6 0 -1.5707 0 0</pose>
                <inertial>
                    <mass>1</mass>
                    <inertia>
                        <ixx>0.013333</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.013333</iyy>
                        <iyz>0</iyz>
                        <izz>0.02</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <cylinder>
                            <radius>0.2</radius>
                            <length>0.2</length>
                        </cylinder>
                    </geometry>
                    <material>
                        <ambient>0.5 0.5 0.7 1</ambient>
                        <diffuse>0.5 0.5 0.7 1</diffuse>
                        <specular>0.5 0.5 0.7 1</specular>
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <cylinder>
                            <radius>0.2</radius>
                            <length>0.2</length>
                        </cylinder>
                    </geometry>
                </collision>
            </link>
            <link name='back_right_wheel'>
                <pose relative_to="chassis">0.4 -0.6 0 -1.5707 0 0</pose> <!--angles are in radian-->
                <inertial>
                    <mass>1</mass>
                    <inertia>
                        <ixx>0.013333</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.013333</iyy>
                        <iyz>0</iyz>
                        <izz>0.02</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <cylinder>
                            <radius>0.2</radius>
                            <length>0.2</length>
                        </cylinder>
                    </geometry>
                    <material>
                        <ambient>0.5 0.5 0.7 1</ambient>
                        <diffuse>0.5 0.5 0.7 1</diffuse>
                        <specular>0.5 0.5 0.7 1</specular>
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <cylinder>
                            <radius>0.2</radius>
                            <length>0.2</length>
                        </cylinder>
                    </geometry>
                </collision>
            </link>
            <joint name='left_wheel_joint' type='revolute'>
                <pose relative_to='left_wheel'/>
                <parent>chassis</parent>
                <child>left_wheel</child>
                <axis>
                    <xyz expressed_in="chassis">0 1 0</xyz> <!--can be defined as any frame or even arbitrary frames-->
                    <limit>
                        <lower>-1.79769e+308</lower>    <!--negative infinity-->
                        <upper>1.79769e+308</upper>     <!--positive infinity-->
                    </limit>
                </axis>
            </joint>
            <joint name='right_wheel_joint' type='revolute'>
                <pose relative_to='right_wheel'/>
                <parent>chassis</parent>
                <child>right_wheel</child>
                <axis>
                    <xyz expressed_in="chassis">0 1 0</xyz>
                    <limit>
                        <lower>-1.79769e+308</lower>    <!--negative infinity-->
                        <upper>1.79769e+308</upper>     <!--positive infinity-->
                    </limit>
                </axis>
            </joint>
            <joint name='back_left_wheel_joint' type='revolute'>
                <pose relative_to='back_left_wheel'/>
                <parent>chassis</parent>
                <child>back_left_wheel</child>
                <axis>
                    <xyz expressed_in="chassis">0 1 0</xyz> <!--can be defined as any frame or even arbitrary frames-->
                    <limit>
                        <lower>-1.79769e+308</lower>    <!--negative infinity-->
                        <upper>1.79769e+308</upper>     <!--positive infinity-->
                    </limit>
                </axis>
            </joint>
            <joint name='back_right_wheel_joint' type='revolute'>
                <pose relative_to='back_right_wheel'/>
                <parent>chassis</parent>
                <child>back_right_wheel</child>
                <axis>
                    <xyz expressed_in="chassis">0 1 0</xyz>
                    <limit>
                        <lower>-1.79769e+308</lower>    <!--negative infinity-->
                        <upper>1.79769e+308</upper>     <!--positive infinity-->
                    </limit>
                </axis>
            </joint>
            <plugin
                filename="gz-sim-diff-drive-system"
                name="gz::sim::systems::DiffDrive">
                <left_joint>left_wheel_joint</left_joint>
                <right_joint>right_wheel_joint</right_joint>
                <left_joint>back_left_wheel_joint</left_joint>
                <right_joint>back_right_wheel_joint</right_joint>
                <wheel_separation>1.2</wheel_separation>
                <wheel_radius>0.2</wheel_radius>
                <odom_publish_frequency>2</odom_publish_frequency>
                <topic>/cmd_vel</topic>
            </plugin>
            <plugin
                filename="gz-sim-pose-publisher-system"
                name="gz::sim::systems::PosePublisher">
                <publish_link_pose>true</publish_link_pose>
                <use_pose_vector_msg>true</use_pose_vector_msg>
                <static_publisher>true</static_publisher>
                <static_update_frequency>1</static_update_frequency>
            </plugin>
            <plugin
                filename="gz-sim-odometry-publisher-system"
                name="gz::sim::systems::OdometryPublisher">
                <odom_frame>world/odom</odom_frame>
                <robot_base_frame>carrinho</robot_base_frame>
                <odom_publish_frequency>2</odom_publish_frequency>
                <odom_topic>/carrinho/odometry</odom_topic>
            </plugin>
        </model>
    </world>
</sdf>